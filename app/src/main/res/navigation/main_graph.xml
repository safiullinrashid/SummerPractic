<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.summerpractic.MainFragment"
        android:label="MainFragment"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_mapFragment2"
            app:destination="@id/mapFragment2" />
        <action
            android:id="@+id/action_mainFragment_to_defaultFragment"
            app:destination="@id/defaultFragment" />
    </fragment>


    <fragment
        android:id="@+id/defaultFragment"
        android:name="com.example.summerpractic.DefaultFragment"
        android:label="DefaultFragment"
        tools:layout="@layout/fragment_default"/>
    <fragment
        android:id="@+id/mapFragment2"
        android:name="com.example.summerpractic.MapFragment"
        android:label="MapFragment"
        tools:layout="@layout/fragment_map">
        <action
            android:id="@+id/action_mapFragment2_to_galleryFragment2"
            app:destination="@id/galleryFragment2" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.summerpractic.SettingsFragment"
        android:label="SettingsFragment"
        tools:layout="@layout/fragment_settings"/>


    <fragment
        android:id="@+id/profileFragment2"
        android:name="com.example.summerpractic.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment2_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_profileFragment2_to_defaultFragment2"
            app:destination="@id/defaultFragment" />
    </fragment>


    <fragment
        android:id="@+id/galleryFragment2"
        android:name="com.example.summerpractic.GalleryFragment"
        android:label="GalleryFragment"
        tools:layout="@layout/fragment_gallery">
        <action
            android:id="@+id/action_galleryFragment2_to_profileFragment2"
            app:destination="@id/profileFragment2" />
    </fragment>


</navigation>
